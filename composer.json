{
    "name": "tahsilat/tahsilat-php",
    "description": "Tahsilat Payment Gateway PHP SDK",
    "keywords": ["tahsilat", "payment", "gateway", "credit card", "api"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/tahsilat/tahsilat-php",
    "authors": [
        {
            "name": "Tahsilat Development Team",
            "email": "dev@tahsilat.dev"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5 || ^7.5 || ^8.5 || ^9.5"
    },
    "autoload": {
        "psr-4": {
            "Tahsilat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tahsilat\\Tests\\": "tests/"
        }
    }
}